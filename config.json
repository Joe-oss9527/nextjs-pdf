{
  "rootURL": "https://www.anthropic.com/research/how-ai-is-transforming-work-at-anthropic",
  "baseUrl": "https://www.anthropic.com/",
  "pdfDir": "pdfs",
  "concurrency": 5,
  "screenshotDelay": 500,
  "navLinksSelector": "a[href^='/research/']",
  "paginationSelector": "",
  "contentSelector": "main",
  "sectionEntryPoints": [],
  "ignoreURLs": [],
  "maxRetries": 3,
  "retryDelay": 1000,
  "pageTimeout": 45000,
  "browserTimeout": 45000,
  "navigationRetries": 4,
  "imageTimeout": 10000,
  "allowedDomains": [
    "www.anthropic.com"
  ],
  "logLevel": "debug",
  "enablePDFStyleProcessing": true,
  "browser": {
    "headless": true,
    "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/121.0.0.0 Safari/537.36"
  },
  "metadata": {
    "enabled": true,
    "directory": "metadata"
  },
  "output": {
    "finalPdfDirectory": "finalPdf-anthropic",
    "tempDirectory": ".temp"
  },
  "pdf": {
    "engine": "puppeteer",
    "theme": "light",
    "preserveCodeHighlighting": true,
    "enableCodeWrap": true,
    "fontSize": "17px",
    "fontFamily": "system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
    "codeFont": "SFMono-Regular, Consolas, 'Liberation Mono', Menlo, monospace",
    "format": "A4",
    "deviceProfile": "oasis",
    "lineHeight": "1.65",
    "codeFontSize": "14px",
    "maxCodeLineLength": 80,
    "kindleOptimized": true,
    "bookmarks": true,
    "margin": {
      "top": "0.6in",
      "right": "0.4in",
      "bottom": "0.6in",
      "left": "0.4in"
    }
  },
  "python": {
    "executable": "./venv/bin/python",
    "timeout": 300000
  },
  "sectionTitles": {},
  "navigationStrategy": "load",
  "maxPaginationPages": 1,
  "translation": {
    "enabled": true,
    "bilingual": true,
    "targetLanguage": "Chinese",
    "concurrency": 2
  },
  "targetUrls": [
    "https://www.anthropic.com/research/how-ai-is-transforming-work-at-anthropic"
  ]
}