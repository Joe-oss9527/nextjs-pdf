{
  "rootURL": "https://platform.openai.com/docs/guides/prompt-engineering",
  "baseUrl": "https://platform.openai.com/docs",
  "pdfDir": "pdfs",
  "concurrency": 5,
  "screenshotDelay": 500,
  "navLinksSelector": "nav a[href], aside a[href], [role='navigation'] a[href], .sidebar a[href]",
  "contentSelector": "main, article, [role='main'], .main-content",
  "ignoreURLs": [
    "docs/pages",
    "docs/app/api-reference"
  ],
  "maxRetries": 3,
  "retryDelay": 1000,
  "pageTimeout": 45000,
  "browserTimeout": 45000,
  "navigationRetries": 4,
  "imageTimeout": 10000,
  "allowedDomains": [
    "platform.openai.com",
    "openai.com"
  ],
  "logLevel": "info",
  "enablePDFStyleProcessing": false,
  "browser": {
    "headless": true,
    "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/121.0.0.0 Safari/537.36"
  },
  "metadata": {
    "enabled": true,
    "directory": "metadata"
  },
  "output": {
    "finalPdfDirectory": "finalPdf-oasis",
    "tempDirectory": ".temp"
  },
  "pdf": {
    "engine": "puppeteer",
    "theme": "light",
    "preserveCodeHighlighting": true,
    "enableCodeWrap": true,
    "fontSize": "17px",
    "fontFamily": "system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
    "codeFont": "SFMono-Regular, Consolas, 'Liberation Mono', Menlo, monospace",
    "format": "A4",
    "deviceProfile": "oasis",
    "lineHeight": "1.65",
    "codeFontSize": "14px",
    "maxCodeLineLength": 80,
    "kindleOptimized": true,
    "bookmarks": true,
    "margin": {
      "top": "0.6in",
      "right": "0.4in",
      "bottom": "0.6in",
      "left": "0.4in"
    }
  },
  "python": {
    "executable": "./venv/bin/python",
    "timeout": 300000
  }
}
