{
  "rootURL": "https://code.claude.com/docs/en/overview",
  "baseUrl": "https://code.claude.com/docs/en/",
  "pdfDir": "pdfs",
  "concurrency": 5,
  "screenshotDelay": 500,
  "navLinksSelector": "a[href^='/docs/en/'], [id*='sidebar'] a[href], nav a[href]",
  "contentSelector": "#content-area",
  "sectionEntryPoints": [
    "https://code.claude.com/docs/en/overview",
    "https://code.claude.com/docs/en/sub-agents",
    "https://code.claude.com/docs/en/third-party-integrations",
    "https://code.claude.com/docs/en/setup",
    "https://code.claude.com/docs/en/settings",
    "https://code.claude.com/docs/en/cli-reference",
    "https://code.claude.com/docs/en/legal-and-compliance"
  ],
  "ignoreURLs": [],
  "maxRetries": 3,
  "retryDelay": 1000,
  "pageTimeout": 45000,
  "browserTimeout": 45000,
  "navigationRetries": 4,
  "imageTimeout": 10000,
  "allowedDomains": [
    "code.claude.com"
  ],
  "logLevel": "info",
  "enablePDFStyleProcessing": true,
  "browser": {
    "headless": true,
    "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/121.0.0.0 Safari/537.36"
  },
  "metadata": {
    "enabled": true,
    "directory": "metadata"
  },
  "output": {
    "finalPdfDirectory": "finalPdf-oasis",
    "tempDirectory": ".temp"
  },
  "pdf": {
    "engine": "puppeteer",
    "theme": "light",
    "preserveCodeHighlighting": true,
    "enableCodeWrap": true,
    "fontSize": "17px",
    "fontFamily": "system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
    "codeFont": "SFMono-Regular, Consolas, 'Liberation Mono', Menlo, monospace",
    "format": "A4",
    "deviceProfile": "oasis",
    "lineHeight": "1.65",
    "codeFontSize": "14px",
    "maxCodeLineLength": 80,
    "kindleOptimized": true,
    "bookmarks": true,
    "margin": {
      "top": "0.6in",
      "right": "0.4in",
      "bottom": "0.6in",
      "left": "0.4in"
    }
  },
  "python": {
    "executable": "./venv/bin/python",
    "timeout": 300000
  }
}