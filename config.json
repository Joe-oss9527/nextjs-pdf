{
  "rootURL": "https://code.claude.com/docs/en/overview",
  "baseUrl": "https://code.claude.com/docs/en/",
  "pdfDir": "pdfs",
  "concurrency": 5,
  "screenshotDelay": 500,
  "navLinksSelector": "a[href^='/docs/en/'], [id*='sidebar'] a[href], nav a[href]",
  "paginationSelector": "",
  "contentSelector": "#content-area",
  "sectionEntryPoints": [
    "https://code.claude.com/docs/en/overview",
    "https://code.claude.com/docs/en/sub-agents",
    "https://code.claude.com/docs/en/third-party-integrations",
    "https://code.claude.com/docs/en/setup",
    "https://code.claude.com/docs/en/settings",
    "https://code.claude.com/docs/en/cli-reference",
    "https://code.claude.com/docs/en/legal-and-compliance"
  ],
  "ignoreURLs": [],
  "maxRetries": 3,
  "retryDelay": 1000,
  "pageTimeout": 45000,
  "browserTimeout": 45000,
  "navigationRetries": 4,
  "imageTimeout": 10000,
  "allowedDomains": [
    "code.claude.com"
  ],
  "logLevel": "debug",
  "enablePDFStyleProcessing": true,
  "browser": {
    "headless": true,
    "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/121.0.0.0 Safari/537.36"
  },
  "metadata": {
    "enabled": true,
    "directory": "metadata"
  },
  "output": {
    "finalPdfDirectory": "finalPdf-anthropic",
    "tempDirectory": ".temp"
  },
  "pdf": {
    "engine": "puppeteer",
    "theme": "light",
    "preserveCodeHighlighting": true,
    "enableCodeWrap": true,
    "fontSize": "17px",
    "fontFamily": "system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif",
    "codeFont": "SFMono-Regular, Consolas, 'Liberation Mono', Menlo, monospace",
    "format": "A4",
    "deviceProfile": "oasis",
    "lineHeight": "1.65",
    "codeFontSize": "14px",
    "maxCodeLineLength": 80,
    "kindleOptimized": true,
    "bookmarks": true,
    "margin": {
      "top": "0.6in",
      "right": "0.4in",
      "bottom": "0.6in",
      "left": "0.4in"
    }
  },
  "python": {
    "executable": "./venv/bin/python",
    "timeout": 300000
  },
  "sectionTitles": {
    "https://code.claude.com/docs/en/overview": "Getting started",
    "https://code.claude.com/docs/en/sub-agents": "Build with Claude Code",
    "https://code.claude.com/docs/en/third-party-integrations": "Deployment",
    "https://code.claude.com/docs/en/setup": "Administration",
    "https://code.claude.com/docs/en/settings": "Configuration",
    "https://code.claude.com/docs/en/cli-reference": "Reference",
    "https://code.claude.com/docs/en/legal-and-compliance": "Resources"
  },
  "navigationStrategy": "load",
  "maxPaginationPages": 1,
  "translation": {
    "enabled": false,
    "bilingual": true,
    "targetLanguage": "Simplified Chinese (简体中文)",
    "concurrency": 2,
    "timeout": 60000,
    "maxRetries": 3,
    "retryDelay": 2000,
    "maxSegmentRetries": 2,
    "maxDelay": 30000,
    "jitterStrategy": "decorrelated"
  },
  "markdown": {
    "enabled": true,
    "outputDir": "markdown",
    "includeFrontmatter": true
  },
  "markdownPdf": {
    "enabled": true,
    "highlightStyle": "github",
    "pdfOptions": {
      "format": "A4",
      "margin": "20mm"
    }
  },
  "targetUrls": []
}