{
  "name": "nextjs-pdf-scraper",
  "version": "2.0.0",
  "description": "Next.js documentation PDF scraper with improved architecture",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "start:clean": "npm run clean && npm start",
    "clean": "rm -rf pdfs/*/ pdfs/finalPdf/* pdfs/metadata/*",
    "test": "node test-stage8-integration.js",
    "test:demo": "node demo-stage8.js",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js"
  },
  "keywords": [
    "scraper",
    "pdf",
    "nextjs",
    "puppeteer"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.5",
    "joi": "^17.13.3",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^22.4.1",
    "winston": "^3.17.0",
    "p-queue": "^7.4.1",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
